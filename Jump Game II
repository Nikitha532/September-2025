class Solution:
    def jump(self, nums: List[int]) -> int:
        jumps = 0
        curr_end = 0
        fartest = 0
        for i in range (len(nums)-1) :
            fartest = max(fartest, i+nums[i])
            if i == curr_end :
                jumps += 1
                curr_end = fartest
        return jumps
