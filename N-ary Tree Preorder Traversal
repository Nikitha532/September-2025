"""
# Definition for a Node.
class Node:
    def __init__(self, val: Optional[int] = None, children: Optional[List['Node']] = None):
        self.val = val
        self.children = children
"""

class Solution:
    def preorder(self, root: 'Node') -> List[int]:
        def preOrder(root, ans) :
            if(root == None) :
                return
            ans.append(root.val)
            for child in root.children :
                preOrder(child, ans)

        ans = []
        preOrder(root, ans)
        return ans
